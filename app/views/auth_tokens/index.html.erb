<p id="notice"><%= notice %></p>

<h1>Auth Tokens</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Auth token</th>
      <th>Otp</th>
      <th>Device</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_tokens.each do |auth_token| %>
      <tr>
        <td><%= auth_token.number %></td>
        <td><%= auth_token.auth_token %></td>
        <td><%= auth_token.otp %></td>
        <td><%= auth_token.device_id %></td>
        <td><%= auth_token.user %></td>
        <td><%= link_to 'Show', auth_token %></td>
        <td><%= link_to 'Edit', edit_auth_token_path(auth_token) %></td>
        <td><%= link_to 'Destroy', auth_token, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auth Token', new_auth_token_path %>
