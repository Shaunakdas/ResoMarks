<p id="notice"><%= notice %></p>

<h1>Exam Attempts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Exam</th>
      <th>Exam set</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exam_attempts.each do |exam_attempt| %>
      <tr>
        <td><%= exam_attempt.name %></td>
        <td><%= exam_attempt.exam %></td>
        <td><%= exam_attempt.exam_set %></td>
        <td><%= exam_attempt.user %></td>
        <td><%= link_to 'Show', exam_attempt %></td>
        <td><%= link_to 'Edit', edit_exam_attempt_path(exam_attempt) %></td>
        <td><%= link_to 'Destroy', exam_attempt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exam Attempt', new_exam_attempt_path %>
